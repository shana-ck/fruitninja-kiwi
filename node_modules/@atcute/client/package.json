{
  "type": "module",
  "name": "@atcute/client",
  "version": "2.0.4",
  "description": "lightweight and cute API client for AT Protocol",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/mary-ext/atcute",
    "directory": "packages/core/client"
  },
  "files": [
