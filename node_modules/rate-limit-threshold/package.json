{
  "name": "rate-limit-threshold",
  "version": "0.1.5",
  "description": "Rate limiter with a maximum number of calls per given period of time",
  "type": "module",
  "module": "lib/index.js",
  "exports": "./lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*.js",
    "lib/**/*.d.ts"
  ],
  "repository": "https://github.com/Borewit/rate-limit-threshold",
  "author": {
    "name": "Borewit",
    "url": "https://github.com/Borewit"
  },
  "license": "MIT",
  "private": false,
  "engines": {
    "node": "^14.13.1 || >=16.0.0"
  },
  "bugs": {
    "url": "https://github.com/Borewit/rate-limit/issues"
  },
  "devDependencies": {
    "@types/chai": "^4.3.10",
    "@types/debug": "^4.1.12",
    "@types/mocha": "^10.0.4",
    "@types/node": "^20.9.2",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "c8": "^8.0.1",
    "chai": "^4.3.10",
    "del-cli": "^5.1.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsdoc": "^46.9.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-unicorn": "^49.0.0",
    "mocha": "^10.2.0",
    "remark-cli": "^12.0.0",
    "remark-preset-lint-recommended": "^6.1.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "clean": "del-cli lib/**/*.js lib/**/*.js.map lib/**/*.d.ts test/**/*.js test/**/*.js.map",
    "compile-lib": "tsc -p lib",
    "compile-test": "tsc -p test",
    "compile": "npm run compile-lib && npm run compile-test",
    "eslint": "eslint lib/**/*.ts --ignore-pattern lib/**/*.d.ts test/**/*.ts",
    "lint-md": "remark -u preset-lint-recommended .",
    "lint": "npm run lint-md && npm run eslint",
    "test": "mocha",
    "build": "npm run clean && npm run compile",
    "start": "npm-run-all compile lint cover-test",
    "test-coverage": "c8 npm run test",
    "send-codacy": "nyc report --reporter=text-lcov | codacy-coverage"
  },
  "keywords": [
    "ratelimiter",
    "rate",
    "limit",
    "rolling",
    "limitting",
    "request"
  ]
}
